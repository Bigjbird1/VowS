{
  "framework": "nextjs",
  "buildCommand": "cd vowswap && npm run build",
  "outputDirectory": "vowswap/.next/standalone",
  "devCommand": "cd vowswap && npm run dev",
  "installCommand": "cd vowswap && npm install",
  "env": {
    "NEXTAUTH_URL": "https://vowswap.vercel.app",
    "NODE_ENV": "production",
    "CRON_SECRET": "04cb037170153f8c30bf9ed8023dcabf2942d80511eef1838149c7224de625be"
  },
  "regions": ["iad1"],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        },
        {
          "key": "Referrer-Policy",
          "value": "strict-origin-when-cross-origin"
        },
        {
          "key": "Permissions-Policy",
          "value": "camera=(), microphone=(), geolocation=(), interest-cohort=()"
        }
      ]
    }
  ],
  "crons": [
    {
      "path": "/api/cron/cleanup-expired-sessions",
      "schedule": "0 0 * * *"
    },
    {
      "path": "/api/cron/database-backup",
      "schedule": "0 4 * * *"
    }
  ]
}
