{
  "framework": "nextjs",
  "buildCommand": "cd vowswap && npm run build",
  "outputDirectory": "vowswap/.next/standalone",
  "devCommand": "cd vowswap && npm run dev",
  "installCommand": "cd vowswap && npm install",
  "env": {
    "NEXTAUTH_URL": "https://vowswap.vercel.app",
    "NODE_ENV": "production",
    "CRON_SECRET": "04cb037170153f8c30bf9ed8023dcabf2942d80511eef1838149c7224de625be"
  },
  "regions": ["iad1"],
  "build": {
    "env": {
      "NEXTAUTH_URL": "$NEXTAUTH_URL",
      "CRON_SECRET": "$CRON_SECRET",
      "SENTRY_ORG": "$SENTRY_ORG",
      "SENTRY_PROJECT": "$SENTRY_PROJECT"
    }
  },
  "crons": [
    {
      "path": "/api/cron/cleanup-expired-sessions",
      "schedule": "0 0 * * *"
    },
    {
      "path": "/api/cron/database-backup",
      "schedule": "0 4 * * *"
    }
  ]
}
